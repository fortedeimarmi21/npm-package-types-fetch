{
  "name": "npm-package-types-fetch",
  "version": "0.0.8",
  "description": "fetch type definition files for npm package",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "engines": {
    "node": ">= 12.13 <13"
  },
  "files": [
